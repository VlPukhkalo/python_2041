   "source": [
    "s=input(\"Введите строку: \")\n",
    "s=list(s.lower())\n",
    "alpha=[i for i in s if i.isalpha()]\n",
    "z=False\n",
    "for i in range(len(alpha)):\n",
    "    if alpha[i]==alpha[-i-1]:\n",
    "        if i>len(alpha)/2:\n",
    "            z=True\n",
    "            break\n",
    "        else:\n",
    "            i+=1\n",
    "    else:\n",
    "        break\n",
    "print(z)"
   ]
  }
